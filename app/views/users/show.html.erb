<h1>Users#show</h1>
<p>Find me in app/views/users/show.html.erb</p>

<div>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_profile_image(100,100) %>
</div>

<% @books.each do |book| %>
<div>
  <%= link_to book_image_path(book.id) do %>
    <image_tag book.get_image %>
  <% end %>
<% end %>
</div>
</div>
